                        room_type VARCHAR(255),
                        Price_USD INT,
                        sqft INT);
                          
create table bookingcom_room (ID INT PRIMARY KEY  NOT NULL, 
                        room_type VARCHAR(255),
                        Price_USD INT,
                        sqft INT);   

